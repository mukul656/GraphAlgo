#include<bits/stdc++.h>
using namespace std;
void bfs(int src , vector<vector<int>>& adj , vector<int>& vis , vector<int>& ans){
    vis[src] = 1;
    queue<int> q;
    q.push(src);

    while(!q.empty()){
        int node = q.front();
        q.pop();
        ans.push_back(node);
        for(auto it : adj[node]){
            if(!vis[it]){
                vis[it] = 1;
                q.push(it);
            }
        }
    }
}
int main(){
    int n , m;
    cin>>n>>m;
    vector<vector<int>> adj(n);
    for(int i = 0; i < m; i++){
        int u , v;
        cin>>u>>v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }
    vector<int> ans;
    vector<int> vis(n , 0);
    for(int i = 0; i < n; i++) {
        if(!vis[i]) {
            bfs(i, adj, vis, ans);
        }
    }
    return 0;
}
