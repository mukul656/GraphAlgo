#include<bits/stdc++.h>
using namespace std;
void dfs(int node , vector<vector<int>>& adj , vector<int>& vis , vector<int>& arr){
    vis[node] = 1;
    arr.push_back(node);
    for(auto it : adj[node]){
        if(!vis[it]){
            dfs(it , adj , vis , arr);
        }
    }
}
int main(){
    int n , m;
    cin>>n>>m;
    vector<vector<int>> adj(n);
    for(int i = 0; i < m; i++){
        int u , v;
        cin>>u>>v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }
    vector<int> vis(n , 0);
    vector<int> arr;
    for(int i = 0; i < n; i++){
        if(!vis[i]){
            dfs(i , adj , vis , arr);
        }
    }
    for(auto it : arr) cout<<it<<" ";
    return 0;
}
