#include<bits/stdc++.h>
using namespace std;
int main(){
    int n , m;
    cin>>n>>m;
    vector<vector<int>> adj(n);
    for(int i = 0; i < m; i++){
        int u , v;
        cin>>u>>v;
        adj[u].push_back(v);
    }
    vector<int> in(n , 0);
    for(int i = 0; i < n; i++){
        for(auto it : adj[i]){
            in[it]++;
        }
    }
    queue<int> q;
    for(int i = 0; i < n; i++){
        if(in[i] == 0) q.push(i);
    }
    vector<int> ans;
    while(!q.empty()){
        int node = q.front();
        q.pop();
        ans.push_back(node);
        for(auto it : adj[node]){
            in[it]--;
            if(in[it] == 0) q.push(it);
        }
    }
    for(auto it : ans) cout<<it<<" ";
    return 0;
}
